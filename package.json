{
	"name": "normalize-url",
	"version": "7.0.0",
	"description": "Normalize a URL",
	"license": "MIT",
	"repository": "sindresorhus/normalize-url",
	"funding": "https://github.com/sponsors/sindresorhus",
	"author": {
		"name": "Sindre Sorhus",
		"email": "sindresorhus@gmail.com",
		"url": "https://sindresorhus.com"
	},
	"type": "module",
	"exports": "./index.js",
	"engines": {
		"node": ">=12.20"
	},
	"scripts": {
		"test": "xo && npm run build && c8 mocha && karma start karma.conf.cjs && tsd && npm run browser:help",
		"build": "rollup -c",
		"style:fix": "xo --fix index.js test.js",
		"browser:help": "echo 'For real browser test, call\n  npm run browser:test'",
		"browser:test": "npx -y http-server -o browser-test"
	},
	"files": [
		"index.js",
		"index.d.ts"
	],
	"keywords": [
		"normalize",
		"url",
		"uri",
		"address",
		"string",
		"normalization",
		"normalisation",
		"query",
		"querystring",
		"simplify",
		"strip",
		"trim",
		"canonical"
	],
	"devDependencies": {
		"rollup": "^2.56.1",
		"rollup-plugin-terser": "^7.0.2",
		"@rollup/plugin-alias": "^3.1.5",
		"@rollup/plugin-babel": "^5.3.0",
		"c8": "^7.7.3",
		"chai": "^4.3.4",
		"http-server": "^13.0.0",
		"karma": "^6.3.4",
		"karma-chai": "^0.1.0",
		"karma-chrome-launcher": "^3.1.0",
		"karma-firefox-launcher": "^2.1.1",
		"karma-mocha": "^2.0.1",
		"mocha": "^9.0.3",
		"tsd": "^0.17.0",
		"typescript": "^4.3.5",
		"xo": "^0.41.0"
	},
	"c8": {
		"reporter": [
			"text",
			"lcov"
		]
	},
	"xo": {
		"ignores": [
			"browser-test/tests.spec.js"
		]
	}
}
